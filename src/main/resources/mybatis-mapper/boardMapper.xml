<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.kro.namohagae.board.dao.BoardDao">

    <insert id="insertData" parameterType="kr.kro.namohagae.board.dao.Board">
        insert into board (
        board_no,board_title,board_count,board_write_date,board_read_count,board_recommend_count
        )
        values
        (#{board_no},#{board_title},#{board_count},sysdate,0,0)
    </insert>
</mapper>